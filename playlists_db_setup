from setup import *
def excel_list_to_df():
    '''Create the dataframe from the excel sheet of playlists'''

    df = pd.read_excel("Spotify Playlists.xlsx")
    playlists = df['Link']
    return playlists
    
def convert_to_df(playlists):
    for playlist in playlists:
        extract_songs()
    
    


